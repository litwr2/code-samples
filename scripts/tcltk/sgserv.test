area 320 200 darkgreen
circle c1 120 120 20 cyan
line l1 10 10 100 100 magenta
circumference cc1 120 120 10 red
text t1 50 50 green "Hello, World!"
pause
move c1 20 20
move l1 10 10
move cc1 -10 0
move t1 0 -15
pause
area 640 400 darkgreen
setAxes 4 -20 10  5 -10 40  black
showFunc -18.5 11 11*sin(x)/(x) "11*sin x/x" red
showFunc -18.5 11 exp(4*sin((x)-1))-5 exp(4*sin(x-1))-5 darkblue
showFunc -20.41 10.5 7/(x) 7/x lightblue
showAxes
pause
delete func
delete axes
area 800 600 darkgreen
setAxes 1 -3.5 3.5 0.5 -1 1 black
showFunc -3.1416 3.1416 sin(x) sin(x) red
showAxes
pause
delete func
delete axes
setAxes 1 -4 4 1 -9 9 black
showFunc -3.14 3.14 sin(x) sin(x) red
showAxes
pause
